[
  {
    "general":{
      "name": "violet",
      "build-host": "GitHub-Action",
      "builder-name": "magojohnji",
      "enableKprobes": false,
      "enableOverlayFS": false,
      "disable-lto": false,
      "disable-cc-werror": false,
      "release": true,
      "remove-unused-packages": true
    },

    "kernel": {
      "basic": {
        "link": "https://github.com/magojohnji/ancient-kernel_xiaomi_violet.git",
        "branch": "14",
        "defconfig": "vendor/violet-perf_defconfig",
        "arch": "arm64",
        "image": "Image.gz-dtb",
        "need-dtbo": true
      },

      "patches": {
        "legacy-hook.patch"
      }
    },

    "withKernelSU": {
      "enable": false,
      "installer": "",
      "tag": "",
      "apply-patch": false,
      "ksu_expected_size": "",
      "ksu_expected_hash": "",
      "make_bootimg": false,
      "bootimg_link": ""
    },

    "toolchains": [
      {
        "url": "https://github.com/ZyCromerZ/Clang/releases/download/19.0.0git-20240511-release/Clang-19.0.0git-20240511.tar.gz",
        "name": "clang",
        "binaryEnv":[
          "bin"]
      }
    ],

    "ccache": true,

    "enableLXC": false,

    "params": {
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "LLVM": "1",
        "LLVM_IAS": "1"
      }
    },

    "AnyKernel3": {
      "enable": true,
      "link": "https://github.com/rezaadi0105/AnyKernel3.git",
      "branch": "single"
    }
  }
]

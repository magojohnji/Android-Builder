[
  {
    "general":{
      "name": "violet",
      "build-host": "GitHub-Action",
      "builder-name": "magojohnji",
      "enableKprobes": true,
      "enableOverlayFS": true,
      "disable-lto": false,
      "disable-cc-werror": false,
      "release": false,
      "debug": false
    },

    "kernelSource": {
      "link": "https://github.com/magojohnji/android_kernel_xiaomi_violet.git",
      "branch": "14",
      "defconfig": "vendor/violet-perf_defconfig",
      "arch": "arm64",
      "need-dtbo": false
    },

    "withKernelSU": {
      "enable": true,
      "installer": "https://raw.githubusercontent.com/tiann/KernelSU/main/kernel/setup.sh",
      "tag": "main",
      "apply-patch": false,
      "ksu_expected_size": "",
      "ksu_expected_hash": "",
      "make_bootimg": true,
      "bootimg_link": "https://raw.githubusercontent.com/magojohnji/magojohnji/main/boot/boot_PE13_violet.img"
    },

    "toolchains": [
      {
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master/clang-r510928.tar.gz",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],

    "ccache": true,

    "enableLXC": false,

    "params": {
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "LLVM": "1",
        "LLVM_IAS": "1"
      }
    },

    "AnyKernel3": {
      "enable": true,
      "link": "https://github.com/rezaadi0105/AnyKernel3",
      "branch": "dynamic"
    }
  }
]

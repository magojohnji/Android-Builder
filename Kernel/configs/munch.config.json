      "toolchains": [
        {
          "url": "https://github.com/ZyCromerZ/Clang/releases/download/19.0.0git-20240302-release/Clang-19.0.0git-20240302.tar.gz",
          "name": "clang",
          "binaryEnv":[
            "bin"]
        },
        {
          "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
